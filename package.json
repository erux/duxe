    "erux",
    "reducer",
    "enhancer"
  ],
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.6.5",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-check-es2015-constants": "^6.6.5",
    "babel-plugin-transform-es2015-arrow-functions": "^6.5.2",
